<h1 id="he">All Users</h1>
<hr/>
<ul class="nav nav-tabs">
<li>
 <%= link_to "Contents", contents_path %>
 </li>
 <li class="active">
  <%= link_to "Users", users_path %>
</li>
</ul>
<div class="row">
<div class="col-md-4 col-md-offset-4 center">
<%= will_paginate %>
</div>
</div>

<% @users.each do |user| %>
<div class="row">
<div class="col-md-4 col-md-offset-4 center">

		<%= image_tag(user.profilepic.url,size: "200x100", class:"upimg") if user.profilepic? %>
			
			<h3><%= link_to "View The User", user_path(user) , class: "btn btn-primary btn-small center" %></h3>
			<h5 class="center">Name: <%= user.username %></h5>
</div>
</div>
<hr/>
<% end %>
<div class="row">
<div class="col-md-4 col-md-offset-4 center">
<%= will_paginate %>
</div>
</div>